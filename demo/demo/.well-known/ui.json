{
    "ui": [
        {
            "operationId": "uploadFile",
            "component": {
                "type": "upload",
                "props": {
                    "url": " http://127.0.0.1:8081/upload_file",
                    "fileType": ".pdf",
                    "maxSize": "10",
                    "text": "上传文件"
                },
                "events": {
                    "upload": [
                        {
                            "action": "sendPrompt",
                            "option": {
                                "msg": "分析{{fileInfo.name}}"
                            }
                        }
                    ]
                }
            }
        }
    ]
}